{
  "info": {
    "name": "QR Code API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Generate QR Code (GET)",
      "request": {
        "method": "GET",
        "header": [
          { "key": "x-rapidapi-host", "value": "your-rapidapi-host", "type": "text" }
        ],
        "url": {
          "raw": "http://localhost:8000/generate-qr?data=https://example.com&file=png&size=400&body_color=%23000000&bg_color=%23FFFFFF&transparent=false&module_style=square&gradient_type=solid&start_color=%23000000&end_color=%23FFFFFF&caption=MyQR&logo_url=https://site.com/logo.png",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["generate-qr"],
          "query": [
            { "key": "data", "value": "https://example.com" },
            { "key": "file", "value": "png" },
            { "key": "size", "value": "400" },
            { "key": "body_color", "value": "#000000" },
            { "key": "bg_color", "value": "#FFFFFF" },
            { "key": "transparent", "value": "false" },
            { "key": "module_style", "value": "square" },
            { "key": "gradient_type", "value": "solid" },
            { "key": "start_color", "value": "#000000" },
            { "key": "end_color", "value": "#FFFFFF" },
            { "key": "caption", "value": "My custom QR" },
            { "key": "logo_url", "value": "https://site.com/logo.png" }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Generate QR Code (POST)",
      "request": {
        "method": "POST",
        "header": [
          { "key": "x-rapidapi-host", "value": "your-rapidapi-host", "type": "text" },
          { "key": "Content-Type", "value": "multipart/form-data", "type": "text" }
        ],
        "body": {
          "mode": "formdata",
          "formdata": [
            { "key": "data", "value": "https://example.com", "type": "text" },
            { "key": "file", "value": "png", "type": "text" },
            { "key": "size", "value": "400", "type": "text" },
            { "key": "body_color", "value": "#000000", "type": "text" },
            { "key": "bg_color", "value": "#FFFFFF", "type": "text" },
            { "key": "transparent", "value": "false", "type": "text" },
            { "key": "module_style", "value": "square", "type": "text" },
            { "key": "gradient_type", "value": "solid", "type": "text" },
            { "key": "start_color", "value": "#000000", "type": "text" },
            { "key": "end_color", "value": "#FFFFFF", "type": "text" },
            { "key": "caption", "value": "My custom QR", "type": "text" },
            { "key": "logo_url", "value": "https://site.com/logo.png", "type": "text" },
            { "key": "logo", "type": "file" }
          ]
        }
      },
      "response": []
    }
  ]
} 